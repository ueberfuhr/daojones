<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.jdt.ui.javaElementFilters">
      <filter
            class="de.ars.daojones.eclipse.jdt.ui.NatureArtifactFilter"
            description="Hides artifacts that are created when adding the DaoJones facet to a Java Project."
            enabled="true"
            id="de.ars.daojones.eclipse.ui.destinationFolderFilter"
            name="DaoJones Artifacts"
            targetId="org.eclipse.jdt.ui.PackageExplorer">
      </filter>
   </extension>
   <extension
         point="org.eclipse.jdt.ui.quickFixProcessors">
      <quickFixProcessor
            class="de.ars.daojones.eclipse.jdt.ui.markers.handler.AbstractMethodInNonAbstractBeanSolutionGenerator"
            id="de.ars.daojones.eclipse.jdt.ui.quickFixProcessor.abstractMethodInNotAbstractBean"
            name="AbstractMethodInNotAbstractBeanQuickfixProcessor">
         <handledMarkerTypes>
            <markerType
                  id="de.ars.daojones.eclipse.jdt.markers.unimplementedMethodInConcreteBean">
            </markerType>
         </handledMarkerTypes>
      </quickFixProcessor>
      <quickFixProcessor
            class="de.ars.daojones.eclipse.jdt.ui.markers.handler.DoubledAnnotedMethodSolutionGenerator"
            id="de.ars.daojones.eclipse.jdt.ui.quickFixProcessor.doubledAnnotatedMethod"
            name="DoubledAnnotatedMethodProcessor">
         <handledMarkerTypes>
            <markerType
                  id="de.ars.daojones.eclipse.jdt.markers.doubledAnnotatedMethod">
            </markerType>
         </handledMarkerTypes>
      </quickFixProcessor>
      <quickFixProcessor
            class="de.ars.daojones.eclipse.jdt.ui.markers.handler.InvalidMethodFormatSolutionGenerator"
            id="de.ars.daojones.eclipse.jdt.ui.quickFixProcessor.invalidMethodFormatProcessor"
            name="Invalid Method Format Processor">
         <handledMarkerTypes>
            <markerType
                  id="de.ars.daojones.eclipse.jdt.markers.invalidGetterSetterFormatMarker">
            </markerType>
            <markerType
                  id="de.ars.daojones.eclipse.jdt.markers.nonAbstractMethodMarker">
            </markerType>
         </handledMarkerTypes>
      </quickFixProcessor>
      <quickFixProcessor
            class="de.ars.daojones.eclipse.jdt.ui.markers.handler.InvalidTypeHierarchySolutionGenerator"
            id="de.ars.daojones.eclipse.jdt.ui.quickFixProcessor.invalidTypeHierarchyProcessor"
            name="Invalid Type Hierarchy Processor">
         <handledMarkerTypes>
            <markerType
                  id="de.ars.daojones.eclipse.jdt.markers.invalidTypeHierarchyMarker">
            </markerType>
         </handledMarkerTypes>
      </quickFixProcessor>
   </extension>
   <extension
         point="org.eclipse.ui.ide.markerResolution">
      <markerResolutionGenerator
            class="de.ars.daojones.eclipse.jdt.ui.markers.handler.AbstractMethodInNonAbstractBeanSolutionGenerator"
            markerType="de.ars.daojones.eclipse.jdt.markers.unimplementedMethodInConcreteBean">
      </markerResolutionGenerator>
      <markerResolutionGenerator
            class="de.ars.daojones.eclipse.jdt.ui.markers.handler.DoubledAnnotedMethodSolutionGenerator"
            markerType="de.ars.daojones.eclipse.jdt.markers.doubledAnnotatedMethod">
      </markerResolutionGenerator>
      <markerResolutionGenerator
            class="de.ars.daojones.eclipse.jdt.ui.markers.handler.InvalidMethodFormatSolutionGenerator"
            markerType="de.ars.daojones.eclipse.jdt.markers.invalidGetterSetterFormatMarker">
      </markerResolutionGenerator>
      <markerResolutionGenerator
            class="de.ars.daojones.eclipse.jdt.ui.markers.handler.InvalidMethodFormatSolutionGenerator"
            markerType="de.ars.daojones.eclipse.jdt.markers.invalidGetterSetterFormatMarker">
      </markerResolutionGenerator>
      <markerResolutionGenerator
            class="de.ars.daojones.eclipse.jdt.ui.markers.handler.InvalidTypeHierarchySolutionGenerator"
            markerType="de.ars.daojones.eclipse.jdt.markers.invalidTypeHierarchyMarker">
      </markerResolutionGenerator>
   </extension>
