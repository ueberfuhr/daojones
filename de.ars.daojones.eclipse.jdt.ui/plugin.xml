            hasPages="true"
            icon="platform:plugin/org.eclipse.jdt.ui/icons/full/etool16/newclass_wiz.gif"
            id="de.ars.daojones.eclipse.jdt.ui.wizards.newBean"
            name="DaoJones Bean"
