  xsi:schemaLocation="http://maven.apache.org/xsd/assembly-1.1.1.xsd http://maven.apache.org/xsd/assembly-1.1.1.xsd">
  <id>dependencies</id>
  <formats>
    <format>jar</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
