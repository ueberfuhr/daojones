<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      https://maven.apache.org/xsd/settings-1.0.0.xsd">

  <interactiveMode>true</interactiveMode>
  <usePluginRegistry>false</usePluginRegistry>
  <offline>false</offline>
  <servers>
    <server>
      <id>ars-int-release-local</id>
      <username>deployer</username>
      <password>password</password>
    </server>
    <server>
      <id>ars-int-snapshot-local</id>
      <username>deployer</username>
      <password>password</password>
    </server>
    <server>
      <id>ars-ext-release-local</id>
      <username>deployer</username>
      <password>password</password>
    </server>
  </servers>
  <mirrors>
    <mirror>
      <id>repo1-mirror</id>
      <name>ARS Central Repository Mirror</name>
      <url>http://buildart.ars.de:8081/artifactory/repo1</url>
      <mirrorOf>central</mirrorOf>
    </mirror>
  </mirrors>
  <profiles>
    <profile>
      <id>artifactory-repositories</id>
      <repositories>
        <repository>
          <id>ars-int-release-local</id>
          <name>ars-int-release-local</name>
          <url>http://buildart.ars.de:8081/artifactory/ars-int-release-local
          </url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>ars-int-snapshot-local</id>
          <name>ars-int-snapshot-local</name>
          <url>http://buildart.ars.de:8081/artifactory/ars-int-snapshot-local
          </url>
          <snapshots>
            <enabled>true</enabled>
            <updatePolicy>daily</updatePolicy>
          </snapshots>
        </repository>
        <repository>
          <id>ars-ext-release-local</id>
          <name>ars-ext-release-local</name>
          <url>http://buildart.ars.de:8081/artifactory/ars-ext-release-local
          </url>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
      </repositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>artifactory-repositories</activeProfile>
  </activeProfiles>
</settings>